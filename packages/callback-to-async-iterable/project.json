{
  "root": "packages/callback-to-async-iterable",
  "sourceRoot": "packages/callback-to-async-iterable/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/callback-to-async-iterable",
        "main": "packages/callback-to-async-iterable/src/index.ts",
        "tsConfig": "packages/callback-to-async-iterable/tsconfig.lib.json",
        "assets": ["packages/callback-to-async-iterable/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/callback-to-async-iterable/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/callback-to-async-iterable"],
      "options": {
        "jestConfig": "packages/callback-to-async-iterable/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
